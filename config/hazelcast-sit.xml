<hazelcast
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.7.xsd"
        xmlns="http://www.hazelcast.com/schema/config">

    <group>
        <name>sit</name>
        <password>sit-pass</password>
    </group>
    <management-center enabled="false">http://localhost:8080/mancenter</management-center>
    <network>
        <port auto-increment="false" port-count="100">5702</port>
        <join>
            <multicast enabled="false">
            </multicast>
            <tcp-ip enabled="true">
                <interface>192.168.1.51</interface>
                <member-list>
                    <member>192.168.1.51</member>
                </member-list>
            </tcp-ip>
        </join>
    </network>
</hazelcast>
